Main
  css.
    define: !CssDefine
    apply: >css.apply
  html
    .
      html: !Html

CssDefine
  if
    $.+
      : !css-block

CssBlock
  $.+ >css.define()

Html
  html
    .
      body
        $(.*)
          : >html.body
      header
        $(.*)
          : >html.header
      footer
        $(.*)
          : >css.footer
  js.script:
    !Js

Js
  def 
    $(.*)
      : >js.define
    $.+ >main.write
