Main
  css: !Css
  html: !Html

Css
  define: !CssDefine
  apply: >css.apply

CssDefine
  if
    $.+
      : !CssBlock

CssBlock
  $.+ >css.define

Html
  body
    $(.*)
      : >html.body
  head
    $(.*)
      : >html.head
  footer
    $(.*)
      : >html.footer
  js.script:
    !Js

Js
  def 
    $(.*)
      : >js.define
    $.+ >main.write
